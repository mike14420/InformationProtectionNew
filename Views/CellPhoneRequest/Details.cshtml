@model InformationProtection.Models.CellPhoneViewData
@{
    ViewBag.Title = "Details";
    InformationProtection.Models.IpRequestorViewData requestor = (InformationProtection.Models.IpRequestorViewData)(ViewData["requestor"]);
    @Html.Hidden("EmpID", requestor.EmpID, new { id = requestor.EmpID });
    var id = requestor.EmpID;
}

 @Html.ActionLink("Printer Friendly View",
           "Print",
           new { EmpId = id, CellPhoneReqId = Model.CellPhoneReqId },
           null)
<h2 class="RenownHeader1">CELL PHONE USE/REQUEST FORM</h2>
<fieldset>
    <legend>Requestor Information</legend>
    <table style="width: 50%;">
        <tr>
            <td>
                <p>User Form for Employee ID:</p>
            </td>
            <td>@Html.Label(requestor.EmpID as string)</td>
        </tr>
        <tr>
            <td>
                <p>Employee Name:</p>
            </td>
            <td>@Html.Label(requestor.FullName as string)</td>
        </tr>


    </table>

</fieldset>
@RenderPage("~/Views/Shared/_CellPhoneDetails.cshtml")

<table class="Table1Format" style="width: 98%;border:2px solid">
    <tr>
        <td style = "font-size:20px;font-weight:bold;text-align:left" >
            @Html.ActionLink("Back To Users View", "Index", "UsersView", new { EmpId = id }, null)
        </td>

        <td style = "font-size:20px;font-weight:bold;text-align:center" >
            @Html.ActionLink("Edit", "Edit", new { EmpID = id, CellPhoneReqId = Model.CellPhoneReqId }, null)
        </td>
    </tr>
</table>

