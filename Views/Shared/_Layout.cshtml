                        <table border="0" style="background-color: purple; color: #52237F">
                            <tr>
                                <td>HELP</td>
                            </tr>
                            <tr>
                                <td style="text-align: left; vertical-align: top;">
                                    <a href="~/Help">
                                        <img src="@Url.Content("~/images/company.png")" /></a>
                                </td>
                            </tr>
                        </table>
                    </div>
                </div>
            </div>
        </div>
        <div id="categories" style="height: 1000px">
