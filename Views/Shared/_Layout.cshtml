<body>
    <div class="dvMain">
        <div id="layout">
            <div class="dvMainInner">
                <div id="dvHeader">
                    <div id="dvLogo" class="InfoProtectTitle">
                        <table border="0" style="width: 750px;">
                            <tr>
                                <td style="text-align: left;">
                                    <a href="http://www.renown.org/">
                                        <img src="@Url.Content("~/images/stoplight.png")" /></a>
                                </td>
                                <td style="text-align: center">Information Protection Form
                                </td>
                            </tr>


                        </table>

                    </div>
